{
  "name": "circleci-snap-in",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "build:watch": "tsc --watch",
    "prepackage": "npm run build",
    "package": "tar -cvzf build.tar.gz dist package.json package-lock.json .npmrc",
    "start": "ts-node src/main.ts",
    "start:watch": "nodemon src/main.ts",
    "start:production": "node dist/main.js",
    "test:server": "nodemon --watch src --watch test test/main.ts",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --watch"
  },
  "author": "Rushil Venkateswar",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@devrev/typescript-sdk": "^1.1.53",
    "@langchain/groq": "^0.1.3",
    "@types/body-parser": "1.19.5",
    "@types/express": "5.0.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "eslint": "^9.21.0",
    "express": "4.21.2",
    "langchain": "^0.3.19",
    "lint": "^0.8.19",
    "nodemon": "3.1.9",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/jest": "29.5.14",
    "@types/node": "22.15.1",
    "jest": "29.7.0",
    "ts-jest": "29.3.2",
    "typescript": "5.8.3"
  }
}
